<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="application.FormUygulama1Controller">
   <children>
      <VBox prefHeight="400.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="78.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@logo.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <Button fx:id="btn_anamenu" mnemonicParsing="false" onAction="#btn_anamenu_click" prefHeight="40.0" prefWidth="201.0" text="Ana Menü">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btn_iletisim" mnemonicParsing="false" onAction="#btn_iletisim_click" prefHeight="40.0" prefWidth="201.0" text="İletişim">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btn_uyeler" mnemonicParsing="false" onAction="#btn_uyeler_click" prefHeight="40.0" prefWidth="201.0" text="Üyeler">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btn_ayarlar" mnemonicParsing="false" onAction="#btn_ayarlar_click" prefHeight="40.0" prefWidth="201.0" text="Ayarlar">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
            <Button fx:id="btn_cikis" mnemonicParsing="false" onAction="#btn_cikis_click" prefHeight="40.0" prefWidth="201.0" text="Çıkış">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
